<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD  
Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">  
<struts>  
  
<package name="Admin_jsp" extends="struts-default" namespace="/Admin_jsp">  
  
<action name="create_user" class="admin_action.CreateUserAction">  
<result name="success">create_user_success.jsp</result>  
<result name="error">create_user_error.jsp</result>  
  <result name="input">admin_create_user.jsp</result> 

</action>
<action name="modify_user" class="admin_action.ModifyUserAction">  
<result name="success">admin_modify_success.jsp</result>  
<result name="error">admin_modify_error.jsp</result>  
  <result name="input">admin_modify_user.jsp</result> 
  
</action>  
</package>
  



</struts>      